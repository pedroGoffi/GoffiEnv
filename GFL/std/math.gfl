proc square(x: i64): i64 =  x * x;

proc fact(x: i64): i64 {
  var res: i64  = 1;
  var iter: i64 = x;
  while 0 < iter {
    res = res * iter;
    iter = iter - 1;
  }
  return res;
}

proc sum_range(begin: i64, end: i64): i64{
     // formula: (b²+b - a²-a)/2 b = end, a = begin
     return (square(end) + end - square(begin) - begin) / 2;
}


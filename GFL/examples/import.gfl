True      :: 1
False     :: 0
stdout    :: 1
SYS_write :: 1
strlen    :: 6
proc write(fd: i64, ptr: i64, ptr_len: i64): void
  = SYSCALL3(fd, ptr, ptr_len, SYS_write);   

proc puts(ptr: i64, ptr_len: i64): void
  = write(stdout, ptr, ptr_len);

proc main(): void {

  var str: *char = "hello\n";
  var x: **char = &str;
  puts(str, strlen);
  *x = "sexx\n";
  
  puts(str, strlen);
  

}

 

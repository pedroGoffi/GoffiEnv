#incluir "std/padrao.gfl"


algoritmo programa_uso(fd: i64, program: *char)
{
  putsf(fd, "USO:\n");
  putsf(fd, program);
  putsf(fd, " INPUT_FP TEXTO\n");
}

algoritmo main(argc: i64, argv: **char)
{  
  // file descriptor
  se argc != 3
  {
    programa_uso(stdout, argv[0]);
    exit(1);    
  }
  var fd: i64 = fopen(argv[1], "r+");
  se fd < 0
  {
    programa_uso(stdout, argv[0]);
    putsf(stderr, "ERRO: fd < 0\n");
    exit(1);    
  }
  putsf(fd, input("dsa"));
}

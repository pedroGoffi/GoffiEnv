proc fact(x: i64): i64 {
  var res: i64  = 1;
  while 0 < x {
    res = res * x;
    x = x - 1;
  }
  return res;
}

proc main(): void {
  var iter: i64 = 0;
  while iter < 10 {
    var f: i64 = fact(iter);
    iter = iter + 1;
    __print__(f);
  }
  // expeceted output
  // 1
  // 1
  // 2
  // 6
  // 24
  // 120
  // 720
  // 5040
  // 40320
  // 362880

}
